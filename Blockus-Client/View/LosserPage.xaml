<Page x:Class="Blockus_Client.View.LosserPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Blockus_Client.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LosserPage">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock x:Name="Txt_Winner" Text="Winner" Style="{StaticResource Title_Text}" HorizontalAlignment="Center"/>
            <Button Content="Regresar al lobby" Style="{StaticResource Normal_Button}" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Page>
