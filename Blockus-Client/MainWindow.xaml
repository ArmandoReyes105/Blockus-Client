<Window x:Class="Blockus_Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Blockus_Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280"
        WindowStartupLocation="CenterScreen"
        Background="{StaticResource BackgroundColor}">

    <Grid>
        <Image Source="Resources/Images/Blue-Ellipse.png" VerticalAlignment="Top" HorizontalAlignment="Right" Name="Image_BlueEllipse"/>
        <Image Source="Resources/Images/Yellow-Ellipse.png" VerticalAlignment="Bottom" HorizontalAlignment="Left" Name="Image_YellowEllipse"/>

        <Frame Name="Frame_Main" Style="{StaticResource Normal_Frame}" JournalOwnership="OwnsJournal"/>
    </Grid>
    
</Window>
